<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.music125.stock.biz.dal.KchartMapper">

    <sql id="allFields">
        id,operator,operation_type,operation_data,is_delete,gmt_created,gmt_modified
    </sql>

    <insert id="insert" keyProperty="id" keyColumn="id" useGeneratedKeys="true" parameterType="LogDO">
        insert into bc_log(
        <trim suffixOverrides=",">
            <if test="operationType!= null">operation_type,</if>
            <if test="operationData!= null">operation_data,</if>
            is_delete,
            gmt_created,
            gmt_modified,
        </trim>
        )
        values (
        <trim suffixOverrides=",">
            <if test="operationType!= null">#{operationType},</if>
            <if test="operationData!= null">#{operationData},</if>
            0,
            now(),
            now(),
        </trim>
        )
    </insert>

</mapper>